{
  "name": "ralphs-shopify-app-template-node",
  "version": "0.0.0-development",
  "private": true,
  "packageManager": "yarn@3.2.3",
  "license": "UNLICENSED",
  "workspaces": [
    "web/*"
  ],
  "scripts": {
    "build": "shopify app build",
    "c:format": "prettier -c $INIT_CWD",
    "c:format:fix": "prettier -w $INIT_CWD",
    "c:lint": "eslint $INIT_CWD --cache --cache-location node_modules/.cache/.eslintcache",
    "c:lint:ci": "eslint $INIT_CWD",
    "c:lint:fix": "eslint $INIT_CWD --cache --cache-location node_modules/.cache/.eslintcache --fix",
    "c:pkg:deps": "syncpack list-mismatches && syncpack lint-semver-ranges",
    "c:pkg:deps:fix": "syncpack fix-mismatches && syncpack set-semver-ranges",
    "c:pkg:fix": "yarn c:pkg:deps:fix && yarn c:pkg:format:fix",
    "c:pkg:format:fix": "syncpack format",
    "c:test": "yarn c:test:frontend && yarn c:test:backend",
    "c:test:backend": "jest $INIT_CWD/backend --cache --passWithNoTests",
    "c:test:backend:ci": "jest $INIT_CWD --cache --passWithNoTests --ci -i",
    "c:test:backend:watch": "jest $INIT_CWD --cache --passWithNoTests --watch",
    "c:test:frontend": "vitest --reporter=verbose",
    "c:type": "cd $INIT_CWD && tsc -b",
    "deploy": "shopify app deploy",
    "dev": "shopify app dev",
    "gen:tsconfig": "ts-node scripts/generators/tsconfig.ts",
    "info": "shopify app info",
    "scaffold": "shopify app scaffold",
    "shopify": "shopify",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@apollo/client": "3.12.7",
    "@shopify/app": "3.16.3",
    "@shopify/app-bridge": "3.7.3",
    "@shopify/app-bridge-react": "3.2.5",
    "@shopify/app-bridge-utils": "3.2.5",
    "@shopify/cli": "3.16.3",
    "@shopify/polaris": "10.6.0",
    "@shopify/shopify-api": "6.2.0",
    "@vitejs/plugin-react": "2.1.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cross-env": "7.0.3",
    "dotenv": "16.0.2",
    "express": "4.18.1",
    "graphql": "16.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "serve-static": "1.15.0",
    "vite": "3.1.6"
  },
  "devDependencies": {
    "@axe/common": "0.1.0",
    "@babel/core": "7.18.13",
    "@babel/eslint-parser": "7.18.9",
    "@babel/preset-env": "7.18.10",
    "@babel/preset-typescript": "7.18.6",
    "@graphql-eslint/eslint-plugin": "3.10.7",
    "@types/eslint": "8.4.6",
    "@types/glob": "8.0.0",
    "@types/node": "18.7.14",
    "@types/shelljs": "0.8.11",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unicorn": "43.0.2",
    "glob": "8.0.3",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3",
    "sass": "1.54.8",
    "semantic-release": "^19.0.5",
    "shelljs": "0.8.5",
    "syncpack": "8.2.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "tslib": "2.4.0",
    "typescript": "4.8.2",
    "typescript-plugin-css-modules": "3.4.0",
    "vitest": "0.22.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Crunchyman-ralph/shopify-app-node-monorepo-express-vite.git"
  }
}
